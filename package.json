{
  "name": "formfiller",
  "version": "2.4.1",
  "description": "Fills fake-data to HTML Form with AI-powered detection. Supports multiple languages.",
  "main": "background.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --fix --ext .js,.ts",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,md}\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "dev": "vite build --watch --mode development",
    "build": "vite build && vite build --config vite.background.config.mts && vite build --config vite.content.config.mts",
    "build:prod": "NODE_ENV=production npm run build",
    "build:analyze": "NODE_ENV=production npm run build -- --mode analyze",
    "build:ts": "tsc",
    "build:chrome": "./build-chrome.sh",
    "build:firefox": "./build-firefox.sh",
    "build:safari": "./build-safari.sh",
    "build:all": "npm run build:chrome && npm run build:firefox && npm run build:safari",
    "prebuild:all": "npm run build:prod",
    "pretest": "npm run lint"
  },
  "keywords": [
    "japanese filler",
    "chinese filler",
    "vietnamese filler",
    "faker",
    "data",
    "filler",
    "form",
    "form filler",
    "formfiller",
    "fake data",
    "fake data generator",
    "fake data generator for form",
    "fake data for form",
    "fake filler"
  ],
  "author": "thuyydt",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.28.5",
    "@types/chrome": "^0.1.31",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "babel-jest": "^30.2.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.0.3",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-static-copy": "^3.1.4"
  },
  "dependencies": {
    "lit": "^3.3.1"
  }
}
